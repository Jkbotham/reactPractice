{
  "name": "reactpractice",
  "version": "1.0.0",
  "description": "Practice creating different website types using React",
  "main": "server.js",
  "engines": {
    "node": "16.13.0",
    "npm": "8.1.0"
  },
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "if-env NODE_ENV=production && npm run start:prod || npm run start:dev",
    "start:prod": "node server.js",
    "start:dev": "concurrently \"nodemon --ignore 'client/*'\" \"npm run client\"",
    "client": "cd client && npm run start",
    "seed": "node seeds/seeds.js",
    "install": "cd client && npm install",
    "build": "cd client && npm run build",
    "heroku-postbuild": "cd client/ && npm install --only=dev && NODE_ENV=development npm install --no-shrinkwrap && npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jkbotham/reactPractice.git"
  },
  "author": "James Botham",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Jkbotham/reactPractice/issues"
  },
  "homepage": "https://github.com/Jkbotham/reactPractice#readme",
  "dependencies": {
    "axios": "^0.24.0",
    "body-parser": "^1.19.0",
    "concurrently": "^6.4.0",
    "dotenv": "^8.2.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "if-env": "^1.0.4",
    "mongoose": "^5.9.26",
    "morgan": "^1.10.0",
    "path": "^0.12.7",
    "react-router-dom": "^5.3.0",
    "react-scripts": "^4.0.3"
  },
  "devDependencies": {
    "nodemon": "^2.0.4"
  }
}
